# Tide CMake config file
#
# This file sets the following variables:
# Tide_FOUND - Always TRUE.
# Tide_INCLUDE_DIRS - Directories containing the Tide include files.
# Tide_LIBRARIES - Libraries needed to use Tide.
# Tide_DEFINITIONS - Compiler flags for Tide.
# Tide_VERSION - The version of Tide found.
# Tide_VERSION_MAJOR - The major version of Tide found.
# Tide_VERSION_MINOR - The minor version of Tide found.
# Tide_VERSION_REVISION - The revision version of Tide found.
# Tide_VERSION_CANDIDATE - The candidate version of Tide found.

message(STATUS "Found Tide-@PROJECT_VERSION@")
set(Tide_FOUND TRUE)

set(Tide_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@")

if(WIN32)
    set(Tide_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@")
else(WIN32)
    set(Tide_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@")
endif(WIN32)

set(Tide_DEFINITIONS )

set(Tide_VERSION @PROJECT_VERSION@)
set(Tide_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(Tide_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(Tide_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(Tide_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

